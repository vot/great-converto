<h2>Job #{{jobId}}</h2>

<p>File: {{data.fileBasename}}</p>
<p>Output: {{data.format}}</p>
<p>State: {{data.state}}</p>

{{#if data.url}}
  <p><a href="{{data.url}}" class="btn btn-md btn-primary">Download</a></p>
{{else}}
  <p>Progress: {{#if data.progress}}{{data.progress}}{{else}}0{{/if}}%</p>
  <meta http-equiv="refresh" content="3">
{{/if}}
